config_version=5

[application]

config/name="指向黑块"
config/name_localized={
"zh_CN": "指向黑块",
"zh_TW": "指向黑塊"
}
config/description="三维的别踩白块游戏"
config/version="0.0.3_alpha"
run/main_scene="res://场景/根场景.tscn"
config/auto_accept_quit=false
config/quit_on_go_back=false
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"
config/macos_native_icon="res://icon.svg"
config/windows_native_icon="res://icon.svg"

[audio]

buses/default_bus_layout=""
general/text_to_speech=true
driver/enable_input=true

[autoload]

"全局脚本"="*res://脚本/全局脚本.gd"

[debug]

file_logging/enable_file_logging=true

[display]

window/size/initial_position_type=2
window/handheld/orientation=6
display_server/assembly_name="3"

[file_customization]

folder_colors={
"res://场景/": "red",
"res://字体/": "orange",
"res://插件/": "green",
"res://文字/": "teal",
"res://模型/": "blue",
"res://着色/": "purple",
"res://纹理/": "pink",
"res://脚本/": "gray",
"res://音乐/": "yellow"
}

